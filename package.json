{
  "name": "dp05-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .jsx,.js,.json",
    "lint:fix": "eslint src --ext .jsx,.js,.json --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "prettier --check . && eslint src --ext .jsx,.js,.json",
    "format:fix": "prettier --write . && eslint src --ext .jsx,.js,.json --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "axios": "^1.3.5",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.2.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "sass": "^1.59.3",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "commitlint": "^16.0.1",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-order-alphabetical": "^1.0.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "vite": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(jsx|js|json|css|html)": [
      "precheck"
    ]
  }
}
